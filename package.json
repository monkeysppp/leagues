{
  "name": "Leagues",
  "version": "0.0.3",
  "description": "Leagues API",
  "private": true,
  "license": "MIT",
  "proxy": "https://localhost:3001",
  "homepage": "https://localhost:3001/ui",
  "scripts": {
    "start": "node ./leagues run",
    "pretest": "npm run lint",
    "test": "npm run test-unit",
    "test-unit": "nyc --reporter=html --reporter=text mocha \"test/**/*-test.js\" --exit",
    "posttest": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test-no-coverage": "mocha \"test/**/*-test.js\" --exit",
    "lint": "standard --verbose",
    "lint-fix": "standard --verbose --fix",
    "postlint": "npm run doc",
    "doc": "jsdoc --configure jsdoc.json -P ./package.json -R ./README.md .",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test",
    "eject-react": "react-scripts eject",
    "build-release-bundle": "npm test && bash ./bundleRelease.sh",
    "install-release": "npm install && npm run build-react"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@testing-library/jest-dom": "^5.11.1",
    "@testing-library/react": "^10.4.7",
    "@testing-library/user-event": "^12.0.14",
    "axios": "^0.19.2",
    "body-parser": "^1.0.0",
    "bunyan": "^1.8.14",
    "commander": "^5.0.0",
    "cookie-parser": "^1.0.0",
    "cron": "^1.0.0",
    "date-fns": "^2.15.0",
    "express": "^4.0.0",
    "helmet": "^3.23.3",
    "inquirer": "^7.3.3",
    "js-yaml": "^3.3.0",
    "jsonwebtoken": "^8.0.0",
    "mysql": "^2.0.0",
    "nocache": "^2.0.0",
    "nodemailer": "^6.4.10",
    "notistack": "^0.9.17",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "universal-cookie": "^4.0.0",
    "uuid4": "^2.0.0"
  },
  "devDependencies": {
    "chai": "latest",
    "chai-as-promised": "latest",
    "jsdoc": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "proxyquire": "latest",
    "sinon": "latest",
    "sinon-chai": "latest",
    "standard": "latest",
    "supertest": "latest"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "src/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
