{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "source": {
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/\\\\)_|node_modules|doc|jsdoc|coverage"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "MonospaceLinks": false
  },
  "opts": {
    "encoding": "utf8",
    "destination": "jsdoc",
    "recurse": true,
    "private": true,
    "pedantic": true
  }
}
