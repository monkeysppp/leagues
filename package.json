{
  "name": "leagues",
  "version": "1.0.5",
  "description": "Leagues API",
  "private": true,
  "license": "MIT",
  "proxy": "https://localhost:3001",
  "homepage": "https://localhost:3001/ui",
  "scripts": {
    "start": "node ./leagues run",
    "pretest": "npm run lint",
    "test": "npm run test-unit",
    "test-unit": "nyc --reporter=html --reporter=text mocha \"test/**/*-test.js\" --exit",
    "posttest": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test-no-coverage": "mocha \"test/**/*-test.js\" --exit",
    "lint": "standard --verbose",
    "lint-fix": "standard --verbose --fix",
    "postlint": "npm run doc",
    "doc": "jsdoc --configure jsdoc.json -P ./package.json -R ./README.md .",
    "start-react": "react-scripts start",
    "build-react": "react-scripts build",
    "test-react": "react-scripts test",
    "eject-react": "react-scripts eject",
    "build-release-bundle": "npm test && bash ./bundleRelease.sh",
    "install-release": "npm ci && npm run build-react"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "body-parser": "^1.0.0",
    "bunyan": "^1.8.15",
    "commander": "^8.1.0",
    "cookie-parser": "^1.0.0",
    "cron": "^1.0.0",
    "date-fns": "^2.23.0",
    "express": "^4.0.0",
    "helmet": "^4.6.0",
    "inquirer": "^7.3.3",
    "jsonwebtoken": "^8.0.0",
    "mysql": "^2.0.0",
    "nocache": "^2.0.0",
    "nodemailer": "^6.6.3",
    "notistack": "^1.0.10",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "password-hash-and-salt": "^0.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3",
    "universal-cookie": "^4.0.4",
    "uuid4": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "jsdoc": "^3.6.7",
    "mocha": "^9.1.0",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "standard": "^16.0.3",
    "supertest": "^6.1.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": [
      "src/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
